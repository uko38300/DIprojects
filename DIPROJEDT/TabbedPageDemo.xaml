<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DIPROJEDT.TabbedPageDemo"
             Title="TabbedPageDemo"
             BarBackground="DarkBlue"
             BarTextColor="LightSalmon"
             UnselectedTabColor="DarkKhaki">
    <ContentPage Title="Página 1" IconImageSource="dotnet_bot.png">
        <VerticalStackLayout>
            <Image Source="dotnet_bot.png" WidthRequest="50" HeightRequest="50"/>
        </VerticalStackLayout>
    </ContentPage>
    <ContentPage Title="Página 2" IconImageSource="dotnet_bot.png">
        <Label Text="Página 2"/>
    </ContentPage>
    <ContentPage Title="Página 3" IconImageSource="dotnet_bot.png">
        <Label Text="Página 3"/>
    </ContentPage>
</TabbedPage>
